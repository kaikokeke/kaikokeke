{
  "projectType": "library",
  "root": "packages/shared/angular",
  "sourceRoot": "packages/shared/angular/src",
  "prefix": "k",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/packages/shared/angular"],
      "options": {
        "project": "packages/shared/angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/shared/angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/shared/angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/shared/angular"],
      "options": {
        "jestConfig": "packages/shared/angular/jest.config.js",
        "passWithNoTests": true,
        "codeCoverage": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/shared/angular/src/**/*.ts", "packages/shared/angular/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:dependencies", "domain:shared", "framework:angular", "type:library"]
}
