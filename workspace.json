{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "standaloneConfig": true
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "buildable": true,
      "publishable": true,
      "simpleModuleName": true,
      "standaloneConfig": true,
      "strict": true,
      "unitTestRunner": "jest"
    },
    "@nrwl/node:library": {
      "buildable": true,
      "linter": "eslint",
      "publishable": true,
      "simpleModuleName": true,
      "standaloneConfig": true,
      "strict": true,
      "testEnvironment": "jsdom",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "angular": "apps/angular",
    "environment-angular": "packages/environment\\angular",
    "environment-common": "packages/environment/common",
    "shared-angular": "packages/shared/angular",
    "shared-common": "packages/shared/common",
    "shared-devtools": "packages/shared/devtools"
  }
}
