{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "angular": {
      "projectType": "application",
      "root": "apps/angular",
      "sourceRoot": "apps/angular/src",
      "prefix": "kaikokeke",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular",
            "index": "apps/angular/src/index.html",
            "main": "apps/angular/src/main.ts",
            "polyfills": "apps/angular/src/polyfills.ts",
            "tsConfig": "apps/angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/angular/src/favicon.ico", "apps/angular/src/assets"],
            "styles": ["apps/angular/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular/src/environments/environment.ts",
                  "with": "apps/angular/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular:build:production"
            },
            "development": {
              "browserTarget": "angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular/src/**/*.ts", "apps/angular/src/**/*.html"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular"],
          "options": {
            "jestConfig": "apps/angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "environment-common": {
      "root": "packages/environment/common",
      "sourceRoot": "packages/environment/common/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/environment/common",
            "tsConfig": "packages/environment/common/tsconfig.lib.json",
            "packageJson": "packages/environment/common/package.json",
            "main": "packages/environment/common/src/index.ts",
            "assets": ["packages/environment/common/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/environment/common/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/environment/common"],
          "options": {
            "jestConfig": "packages/environment/common/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          }
        }
      },
      "tags": [],
      "configFilePath": "packages\\environment\\common/project.json"
    },
    "shared-angular": {
      "projectType": "library",
      "root": "packages/shared/angular",
      "sourceRoot": "packages/shared/angular/src",
      "prefix": "k",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/packages/shared/angular"],
          "options": {
            "project": "packages/shared/angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/shared/angular/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/shared/angular/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/shared/angular"],
          "options": {
            "jestConfig": "packages/shared/angular/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/shared/angular/src/**/*.ts", "packages/shared/angular/src/**/*.html"]
          }
        }
      },
      "configFilePath": "packages\\shared\\common-angular/project.json"
    },
    "shared-common": {
      "root": "packages/shared/common",
      "sourceRoot": "packages/shared/common/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/shared/common",
            "tsConfig": "packages/shared/common/tsconfig.lib.json",
            "packageJson": "packages/shared/common/package.json",
            "main": "packages/shared/common/src/index.ts",
            "assets": ["packages/shared/common/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/shared/common/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/shared/common"],
          "options": {
            "jestConfig": "packages/shared/common/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          }
        }
      },
      "tags": ["scope:dependencies", "scope:shared", "type:library"],
      "configFilePath": "packages\\shared\\common/project.json"
    },
    "shared-devtools": {
      "root": "packages/shared/devtools",
      "sourceRoot": "packages/shared/devtools/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/shared/devtools",
            "tsConfig": "packages/shared/devtools/tsconfig.lib.json",
            "packageJson": "packages/shared/devtools/package.json",
            "main": "packages/shared/devtools/src/index.ts",
            "assets": ["packages/shared/devtools/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/shared/devtools/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/shared/devtools"],
          "options": {
            "jestConfig": "packages/shared/devtools/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          }
        }
      },
      "tags": ["scope:devDependencies", "scope:shared", "type:library"],
      "configFilePath": "packages\\shared\\devtools/project.json"
    }
  }
}
